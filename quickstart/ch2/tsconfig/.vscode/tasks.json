{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "ts-node와 tslint를 동시에 실행해 특정 ts파일의 결과 보기",
			"type": "shell",
			"command": "ts-node ${file} '&' tslint ${file}",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
				"reveal": "always",
				"panel": "new"
			}	
		}
	]
}